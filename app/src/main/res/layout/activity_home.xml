<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFCD00"
    tools:context=".home">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="415dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:background="@drawable/rectangle_1"
        />

    <TextView
        android:id="@+id/ui_acs"
        android:layout_width="175dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="-15dp"
        android:gravity="center_vertical"
        android:text="@string/ui_acs"
        android:textAppearance="@style/ui_acs" />

    <Button
        android:id="@+id/qrcodebuttonhome"
        android:layout_width="298dp"
        android:layout_height="175dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="65dp"
        android:layout_marginRight="65dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="75dp"
        android:text="scan you teacher code or input your number code"
        android:background="@drawable/rectangle_6"
        />

    <View
        android:id="@+id/rectangle_13"
        android:layout_width="415dp"
        android:layout_height="45dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="274dp"
        android:background="@drawable/rectangle_1"
        />
    <TextView
        android:id="@+id/reminder"
        android:layout_width="237dp"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="280dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="265dp"
        android:text="@string/reminder"
        android:textAppearance="@style/reminder"
        android:gravity="center_vertical"
        />

    <Button
        android:id="@+id/kalenderhome"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="389dp"
        android:text="Kalender"
        android:background="@drawable/homerect1"
        />

    <Button
        android:id="@+id/kelashome"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="157dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="389dp"
        android:text="Kelas"
        android:background="@drawable/homerect1"
        />

    <Button
        android:id="@+id/tugashome"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="274dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="389dp"
        android:text="Tugas"
        android:background="@drawable/homerect1"
        />
    <!-- Rectangle 62 -->
    <Button
        android:id="@+id/notehome"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="96dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="497dp"
        android:text="note"
        android:background="@drawable/homerect1"
        />

    <Button
        android:id="@+id/profilehome"
        android:layout_width="86dp"
        android:layout_height="81dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="212dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="497dp"
        android:text="Profile"
        android:background="@drawable/homerect1"
        />
    <Button
        android:id="@+id/logouthome"
        android:layout_width="329dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="615dp"
        android:text="logout"
        android:background="@drawable/homerect2"
        />

    <Button
        android:id="@+id/exitprogram"
        android:layout_width="329dp"
        android:layout_height="38dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="665dp"
        android:text="exit"
        android:background="@drawable/homerect2"
        />




</RelativeLayout>